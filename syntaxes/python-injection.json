{
  "scopeName": "python.injection",
  "injectionSelector": "L:source.yaml",
  "patterns": [
    {
      "include": "#code"
    }
  ],
  "repository": {
    "code": {
      "begin": "(?<=^[\\s]*\\bcode:)",
      "end": "(?=---|\\b\\w*:\\S)",
      "patterns": [
        {
          "include": "source.python"
        }
      ]
    }
  }
}
